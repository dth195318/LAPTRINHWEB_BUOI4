*{
	font-family: Arial;
	font-size: 11pt;
}


th{
	font-size: 16pt;
	background: #0000ff;
	color: #b0e0e6;
	
}

table{
	border-collapse: collapse;
    padding: 5px;
	background:#87cefa;
	
}

input{
	width: 250px;
	padding: 10px;
	border: 1px dotted #7fffd4;
	background: #f0f8ff;
}
button{
	padding: 5px;
	border: 1px solid #0000ff;
	background: #0000ff;
}